echo "Test X: (shortest distance, number of paths)"
echo "TEST 1: (2,2)"
./greedy_robot 1 3 4 4 3
echo "TEST 2: (Already there)"
./greedy_robot 2 1 1 1 1
echo "TEST 3: (NP, 0)"
./greedy_robot 1 1 1 5 1
echo "TEST 4: (4, 6)"
./greedy_robot 2 1 1 -1 -1
echo "TEST 5: (5, 7)"
./greedy_robot 2 -2 -3 0 0
echo "TEST 6: (16, 35)"
./greedy_robot 3 -1 -6 -5 6
echo "TEST 7: (5, N/A)"
./greedy_robot 2 -2 -3 0